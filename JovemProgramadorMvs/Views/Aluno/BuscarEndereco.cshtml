@model EnderecoModel

<form>
    <div class="mb-3">
        <label for="Name" class="form-label">Logradouro</label>
        <input type="text" class="form-control col-md-6" asp-for="logradouro">
    </div>
    <div class="mb-3">
        <label for="Name" class="form-label">Complemento</label>
        <input type="text" class="form-control col-md-3"  asp-for="complemento">
    </div>
    <div class="mb-3">
        <label for="Name" class="form-label">Bairro</label>
        <input type="text" class="form-control col-md-4"  asp-for="bairro">
    </div>
    <div class="mb-3">
        <label for="Name" class="form-label">Localidade</label>
        <input type="text" class="form-control col-md-2" asp-for="localidade">
    </div>
    <div class="mb-3">
        <label for="Name" class="form-label">UF</label>
        <input type="text" class="form-control col-md-2" asp-for="uf">
    </div>
    <div class="mb-3">
        <label for="Name" class="form-label">DDD</label>
        <input type="text" class="form-control col-md-2" asp-for="ddd">
    </div>
</form>